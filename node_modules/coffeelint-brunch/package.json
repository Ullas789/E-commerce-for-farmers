{
  "name": "coffeelint-brunch",
  "version": "1.7.1",
  "description": "Adds CoffeeLint support to brunch.",
  "repository": {
    "type": "git",
    "url": "git@github.com:brunch/coffeelint-brunch.git"
  },
  "main": "./lib/",
  "scripts": {
    "prepublish": "rm -rf lib && coffee --bare --output lib/ src/",
    "test": "node_modules/.bin/mocha --compilers coffee:coffee-script --require test/common.js"
  },
  "dependencies": {
    "coffeelint": ">= 0.4.0"
  },
  "devDependencies": {
    "chai": "*",
    "coffee-script": "*",
    "mocha": "*"
  }
}
